{
  "name": "soundmobx",
  "version": "1.0.0",
  "description": "Soundcloud client using React, Mobx and Sound Cloud APIs",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --colors --config ./webpack.dev.config.js",
    "build": "rm -rf dist; NODE_ENV=production webpack -p --progress --colors --config ./webpack.prod.config.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "repository": {},
  "author": "Shekhar Kumar <shekhar.mnnit10@gmail.com> (http://iamshek.online/)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "chunk-manifest-webpack-plugin": "1.0.0",
    "css-loader": "0.26.2",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "koa": "1.4.0",
    "koa-router": "5.4.0",
    "koa-static": "2.1.0",
    "node-sass": "4.5.0",
    "path": "0.12.7",
    "postcss-loader": "1.3.3",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.2",
    "svg-sprite-loader": "0.3.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-manifest-plugin": "1.1.0"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "mobx": "3.1.0",
    "mobx-react": "4.1.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "soundcloud": "3.1.2",
    "whatwg-fetch": "2.0.2"
  }
}
